{
  "name": "aws-project",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "setup": "rimraf node_modules && bun install",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "biome:check": "biome check .",
    "biome:apply": "biome check . --apply-unsafe",
    "format": "biome format .",
    "typecheck": "tsc"
  },
  "dependencies": {
    "next": "14.2.14",
    "react": "^18",
    "react-dom": "^18",
    "sass": "^1.77.4"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.3",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "simple-git-hooks": "^2.11.1",
    "typescript": "^5"
  },
  "simple-git-hooks": {
    "pre-commit": "bun run biome:apply",
    "pre-push": "bun run typecheck && bun run build"
  }
}
